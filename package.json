{
  "name": "marketchoo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.int -- next dev -p 8080",
    "dev:int": "concurrently \"dotenv -e .env.int -- next dev\" \"cd studio && sanity start\"",
    "dev:prd": "concurrently \"dotenv -e .env.prd -- next dev\" \"cd studio && sanity start\"",
    "build:int": "dotenv -e .env.int -- echo 'Building Sanity to public/studio…' && cd studio && sanity build ../public/studio -y && cd .. && next build",
    "build:prd": "dotenv -e .env.prd -- echo 'Building Sanity to public/studio…' && cd studio && sanity build ../public/studio -y && cd .. && next build",
    "build": "echo 'Building Sanity to public/studio…' && cd studio && sanity build ../public/studio -y && cd .. && next build",
    "start": "concurrently \"next start\" \"cd studio && sanity start\"",
    "postinstall": "lerna bootstrap",
    "check-template": "sanity-template check",
    "lint": "next lint"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@sanity/client": "^2.16.0",
    "@sanity/image-url": "^0.140.22",
    "classnames": "^2.3.1",
    "date-fns": "^2.23.0",
    "firebase": "^8.10.0",
    "formik": "^2.2.9",
    "gray-matter": "^4.0.3",
    "next": "11.1.0",
    "next-sanity": "^0.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebase-hooks": "^3.0.4",
    "react-firebaseui": "^5.0.2",
    "react-share": "^4.4.0",
    "react-use": "^17.2.4",
    "remark": "^14.0.1",
    "remark-html": "^14.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@sanity/cli": "^2.0.9",
    "@types/react": "17.0.19",
    "autoprefixer": "^10.3.3",
    "concurrently": "^6.2.1",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "lerna": "^3.22.1",
    "postcss": "^8.3.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.38.2",
    "tailwindcss": "^2.2.8",
    "typescript": "4.4.2"
  }
}
